<%
const spath = me.split('/')

let module_name = '';
let module_section = '';
let module_item = '';

if (spath.length > 2) {
    module_name = spath[1];
    module_section = spath[2];
    module_item = spath[3];
    // console.log('module_name', `'${module_name}'`, 'module_section', `'${module_section}'`, 'module_item',`'${module_item}'`)
} else {
    module_name = spath[1];
    module_section = '';
}
%>
<div id="kt_aside"
     class="aside aside-dark aside-hoverable"
     data-kt-drawer="true"
     data-kt-drawer-name="aside"
     data-kt-drawer-activate="{default: true, lg: false}"
     data-kt-drawer-overlay="true"
     data-kt-drawer-width="{default:'200px', '300px': '250px'}"
     data-kt-drawer-direction="start"
     data-kt-drawer-toggle="#kt_aside_mobile_toggle">
    <div class="aside-logo flex-column-auto" id="kt_aside_logo">
        <a href="#"><img alt="logo" src="assets/media/logos/logo-ra-lemon.png" class="h-50px logo"/></a>
        <div id="kt_aside_toggle" class="btn btn-icon w-auto px-0 btn-active-color-primary aside-toggle"
             data-kt-toggle="true"
             data-kt-toggle-state="active"
             data-kt-toggle-target="body"
             data-kt-toggle-name="aside-minimize">
            <span class="svg-icon svg-icon-1 rotate-180">
                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24"
                     fill="none">
                    <path d="M11.2657 11.4343L15.45 7.25C15.8642 6.83579 15.8642 6.16421 15.45 5.75C15.0358 5.33579 14.3642 5.33579 13.95 5.75L8.40712 11.2929C8.01659 11.6834 8.01659 12.3166 8.40712 12.7071L13.95 18.25C14.3642 18.6642 15.0358 18.6642 15.45 18.25C15.8642 17.8358 15.8642 17.1642 15.45 16.75L11.2657 12.5657C10.9533 12.2533 10.9533 11.7467 11.2657 11.4343Z"
                          fill="black"/>
                </svg>
            </span>
        </div>
    </div>
    <!-- begin: menu -->
    <div class="aside-menu flex-column-fluid">
        <div class="hover-scroll-overlay-y my-2 py-5 py-lg-8" id="kt_aside_menu_wrapper" data-kt-scroll="true"
             data-kt-scroll-activate="{default: false, lg: true}" data-kt-scroll-height="auto"
             data-kt-scroll-dependencies="#kt_aside_logo, #kt_aside_footer" data-kt-scroll-wrappers="#kt_aside_menu"
             data-kt-scroll-offset="0">
            <div class="menu menu-column menu-title-gray-800 menu-state-title-primary menu-state-icon-primary menu-state-bullet-primary menu-arrow-gray-500"
                 id="#kt_aside_menu" data-kt-menu="true">
                <div data-kt-menu-trigger="click"
                     class="menu-item menu-accordion <%- 'mye' === module_name ? 'show here' : '' %>">
                    <%- include("menu-accordion",{title:'MYE',icon:'bi-people'}) %>
                    <div class="menu-sub menu-sub-accordion menu-active-bg">
                        <%- include("menu-item", {me: `${me}`, href: "/mye/capacitaciones/organizaciones", icon: 'bullet', title:'Registro de Organizaciones'}) %>
                        <%- include("menu-item", {me: `${me}`, href: "/mye/capacitaciones/personas", icon: 'bullet', title:'Registro de Personas'}) %>
                        <%- include("menu-item", {me: `${me}`, href: "/mye/capacitaciones/registro", icon: 'bullet', title:'Registro de Eventos'}) %>
