                                <%- include("menu-item", {me: `${me}`, href: "/catalogo/general/donante", icon: 'bullet', title:'Donante'}) %>
                                <%- include("menu-item", {me: `${me}`, href: "/catalogo/general/proyecto", icon: 'bullet', title:'Proyectos'}) %>
