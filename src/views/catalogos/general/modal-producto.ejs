<div class="modal fade" tabindex="-1" id="kt_modal_producto">
    <div class="modal-dialog">
        <div class="modal-content shadow-none">
            <div class="modal-header">
                <h5 class="modal-title">Nueva Cadena de Valor</h5>
                <!--begin::Close-->
                <div class="btn btn-icon btn-sm btn-active-light-primary ms-2" data-bs-dismiss="modal"
                     aria-label="Close">
                    <i class="bi bi-x fs-3x"></i>
                </div>
                <!--end::Close-->
            </div>
            <div class="modal-body">
                <form action="" method="post" class="row g-3"
                      name="frm-producto" id="frm-producto">
                    <div class="col-sm-12">
                        <div class="form-floating mb-7">
                            <select data-dropdown-parent="#kt_modal_producto"
                                    class="form-control form-control-lg form-select-lg form-select " name="codcadenavalor" id="codcadenavalorp">
                            </select>
                            <label for="codcadenavalorp">Cadena Valor</label>
                        </div>
                    </div>
                    <div class="col-sm-12">
                        <div class="form-floating">
                            <input class="form-control"
                                   placeholder="Producto"
                                   id="strproducto"
                                   required
                                   name="strproducto">
                            <label for="strproducto"><span class="required">Producto</span></label>
                        </div>
                    </div>
                    <div class="col-sm-12">
                        <div class="form-floating">
                            <input class="form-control"
                                   id="codcommcare"
                                   required
                                   name="codcommcare">
                            <label for="codcommcare"><span class="required">Codigo Commcare</span></label>
                        </div>
                    </div>
                    <div class="col-sm-12">
                        <div class="form-check form-switch form-check-custom form-check-solid">
                            <label class="form-check-label required " for="codestadocv">
                                Estado
                            </label>
                            <input class="form-check-input" type="checkbox" value="1" id="codestadocv" name="codestado" checked="checked" />
                        </div>
                    </div>
                    <div class="col-sm-12">
                        <button type="submit"
                                class="btn btn-primary col-sm-12 w-full" id="kt_nuevo_producto">
                            <span class="indicator-label">
                                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-floppy" viewBox="0 0 16 16">
                                    <path d="M11 2H9v3h2z"/>
                                    <path d="M1.5 0h11.586a1.5 1.5 0 0 1 1.06.44l1.415 1.414A1.5 1.5 0 0 1 16 2.914V14.5a1.5 1.5 0 0 1-1.5 1.5h-13A1.5 1.5 0 0 1 0 14.5v-13A1.5 1.5 0 0 1 1.5 0M1 1.5v13a.5.5 0 0 0 .5.5H2v-4.5A1.5 1.5 0 0 1 3.5 9h9a1.5 1.5 0 0 1 1.5 1.5V15h.5a.5.5 0 0 0 .5-.5V2.914a.5.5 0 0 0-.146-.353l-1.415-1.415A.5.5 0 0 0 13.086 1H13v4.5A1.5 1.5 0 0 1 11.5 7h-7A1.5 1.5 0 0 1 3 5.5V1H1.5a.5.5 0 0 0-.5.5m3 4a.5.5 0 0 0 .5.5h7a.5.5 0 0 0 .5-.5V1H4zM3 15h10v-4.5a.5.5 0 0 0-.5-.5h-9a.5.5 0 0 0-.5.5z"/>
                                </svg>
                            </span>
                            <span class="indicator-progress">
                                Por favor espere...
                                <span class="spinner-border spinner-border-sm align-middle ms-2"></span>
                            </span>
                        </button>
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-light" data-bs-dismiss="modal">Cancelar</button>
            </div>
        </div>
    </div>
</div>